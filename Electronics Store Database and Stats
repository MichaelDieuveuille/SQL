--Create an Electronics Store database

CREATE TABLE StoreProducts (id INTEGER PRIMARY KEY, name Text, quantity INTEGER, Rating INTEGER, Price Integer, Refurbished TEXT, New TEXT , SOLD INTEGER);

INSERT INTO StoreProducts VALUES (1, "PS5", 3, 5, 469, "NO", "YES", 3);

