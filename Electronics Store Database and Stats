INSERT INTO StoreProducts VALUES (7, "LG C2 TV", 4, 5, 2800, "NO", "YES", 4);

--Display the entire store products
SELECT * FROM StoreProducts;

--Display the store products in descending order by price
SELECT * FROM StoreProducts where Price >11 order by Price
